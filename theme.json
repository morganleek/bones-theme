{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"customTemplates": [],
	"templateParts": [
		{
			"name": "header",
			"title": "Header",
			"area": "header"
		},
		{
			"name": "footer",
			"title": "Foooter",
			"area": "footer"
		}
	],
	"settings": {
		"appearanceTools": true,
		"color": {
			"duotone": [],
			"gradients": [],
			"palette": [
				{
					"slug": "black",
					"color": "#000000",
					"name": "Black"
				},
				{
					"slug": "white",
					"color": "#ffffff",
					"name": "White"
				},
				{
					"slug": "blue",
					"color": "#0693e3",
					"name": "Blue"
				},
				{
					"slug": "yellow",
					"color": "#fcb900",
					"name": "Yellow"
				}
			]
		},
		"custom": {
			"spacing": {
				"outer": "clamp(1.875rem, -7.1667rem + 14.5833vw, 4.5rem)",
				"rowGap": "clamp(1.5rem, -1.0833rem + 4.1667vw, 2.25rem)"
			},
			"sizing": {
				"narrow": "1280px",
				"wide": "1660px"
			},
			"padding": {
				"horizontal": "2.5rem"
			},
			"typography": {
				"font-size": {
					"heading-1": "clamp(2rem, 1.8750rem + 2.1875vw, 4.5rem)",
					"heading-2": "clamp(1.625rem, 1.3125rem + 0.7813vw, 2.25rem)",
					"heading-3": "clamp(1.375rem, 1.1875rem + 0.4688vw, 1.75rem)",
					"heading-4": "clamp(1.0625rem, 1.0313rem + 0.0781vw, 1.125rem)",
					"heading-5": "1rem",
					"heading-6": "0.875rem",
					"small": "1rem",
					"medium": "1.125rem",
					"large": "1.75rem",
					"x-large": "clamp(1.75rem, 3vw, 2.25rem)"
				},
				"line-height": {
					"heading-1": 1,
					"heading-2": 1.2,
					"heading-3": 1.2,
					"heading-4": 1.2,
					"heading-5": 1.2,
					"heading-6": 1.2,
					"small": 1.2,
					"medium": 1.4,
					"large": 1.4,
					"x-large": 1.4
				}
			}
		},
		"spacing": {
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			],
			"spacingSizes": [
				{
					"name": "1",
					"slug": "20",
					"size": "clamp(0.78125rem, -0.1953rem + 3.125vw, 1.5625rem)"
				},
				{
					"name": "2",
					"slug": "30",
					"size": "clamp(1.171875rem, -0.293rem + 4.6875vw, 2.34375rem)"
				},
				{
					"name": "3",
					"slug": "40",
					"size": "clamp(1.5625rem, -0.3906rem + 6.25vw, 3.125rem)"
				},
				{
					"name": "4",
					"slug": "50",
					"size": "clamp(1.953125rem, -0.4883rem + 7.8125vw, 3.90625rem)"
				},
				{
					"name": "5",
					"slug": "60",
					"size": "clamp(2.34375rem, -0.5859rem + 9.375vw, 4.6875rem)"
				},
				{
					"name": "6",
					"slug": "70",
					"size": "clamp(2.734375rem, -0.6836rem + 10.9375vw, 5.46875rem)"
				},
				{
					"name": "7",
					"slug": "80",
					"size": "clamp(3.125rem, -0.7813rem + 12.5vw, 6.25rem)"
				}
			]
		},
		"typography": {
			"dropCap": false,
			"fontFamilies": [
				{
					"fontFamily": "'Mooli'",
					"name": "Mooli",
					"slug": "mooli",
					"fontFace": [
						{
							"fontFamily": "'Mooli'",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [ "https://fonts.gstatic.com/s/mooli/v1/-F6_fjJpLyk1bbPGDm_6pClI.woff2" ],
							"unicodeRange": "U+0000-00FF, U+0131, U+0152-0153, U+02BB-02BC, U+02C6, U+02DA, U+02DC, U+0304, U+0308, U+0329, U+2000-206F, U+2074, U+20AC, U+2122, U+2191, U+2193, U+2212, U+2215, U+FEFF, U+FFFD"
						}
					]
				},
				{
					"fontFamily": "\"Inter var\"",
					"name": "Inter Var",
					"slug": "inter-var",
					"fontFace": [
						{
							"fontFamily": "Inter Var",
							"fontWeight": "400",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [ "file:./assets/fonts/inter/Inter-upright-var.woff2" ]
						}
					]
				}
			],
			"fontSizes": [
				{
					"fluid": false,
					"size": "var(--wp--custom--typography--font-size--small)",
					"slug": "small"
				},
				{
					"fluid": false,
					"size": "var(--wp--custom--typography--font-size--medium)",
					"slug": "medium"
				},
				{
					"fluid": false,
					"size": "var(--wp--custom--typography--font-size--large)",
					"slug": "large"
				},
				{
					"fluid": false,
					"size": "var(--wp--custom--typography--font-size--x-large)",
					"slug": "x-large"
				}
			]
		},
		"layout": {
			"contentSize": "var(--wp--custom--sizing--narrow)",
			"wideSize": "var(--wp--custom--sizing--wide)"
		}
	},
	"styles": {
		"blocks": {
			"core/button": {
				"border": {
					"radius": "0"
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--white)",
			"text": "var(--wp--preset--color--black)"
		},
		"elements": {
			"h1": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--mooli)",
					"fontWeight": "400",
					"lineHeight": "var(--wp--custom--typography--line-height--heading-1)",
					"fontSize": "var(--wp--custom--typography--font-size--heading-1)",
					"letterSpacing": "0"
				}
			},
			"h2": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--mooli)",
					"fontWeight": "400",
					"lineHeight": "var(--wp--custom--typography--line-height--heading-2)",
					"fontSize": "var(--wp--custom--typography--font-size--heading-2)",
					"letterSpacing": "0"
				}
			},
			"h3": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--mooli)",
					"fontWeight": "400",
					"lineHeight": "var(--wp--custom--typography--line-height--heading-3)",
					"fontSize": "var(--wp--custom--typography--font-size--heading-3)",
					"letterSpacing": "0"
				}
			},
			"h4": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--mooli)",
					"fontWeight": "400",
					"lineHeight": "var(--wp--custom--typography--line-height--heading-4)",
					"fontSize": "var(--wp--custom--typography--font-size--heading-4)",
					"letterSpacing": "0"
				}
			},
			"h5": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--mooli)",
					"fontWeight": "400",
					"lineHeight": "var(--wp--custom--typography--line-height--heading-5)",
					"fontSize": "var(--wp--custom--typography--font-size--heading-5)",
					"letterSpacing": "0"
				}
			},
			"h6": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--mooli)",
					"fontWeight": "400",
					"lineHeight": "var(--wp--custom--typography--line-height--heading-6)",
					"fontSize": "var(--wp--custom--typography--font-size--heading-6)"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--black)"
				}
			}
		},
		"spacing": {
			"blockGap": "1.5rem"
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--inter-var)",
			"lineHeight": "var(--wp--custom--typography--line-height--medium)",
			"fontSize": "var(--wp--preset--font-size--medium)"
		}
	}
}
