						}
					]
				},
				{
					"fontFamily": "\"Inter var\"",
					"name": "Inter Var",
					"slug": "inter-var",
					"fontFace": [
						{
							"fontFamily": "Inter Var",
							"fontWeight": "400",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [ "file:./assets/fonts/inter/Inter-upright-var.woff2" ]
						}
					]
				}
			],
			"fontSizes": [
				{
					"fluid": false,
					"size": "var(--wp--custom--typography--font-size--small)",
					"slug": "small"
				},
				{
					"fluid": false,
					"size": "var(--wp--custom--typography--font-size--medium)",
					"slug": "medium"
				},
				{
					"fluid": false,
					"size": "var(--wp--custom--typography--font-size--large)",
					"slug": "large"
				},
				{
					"fluid": false,
					"size": "var(--wp--custom--typography--font-size--x-large)",
					"slug": "x-large"
				}
			]
		},
		"layout": {
			"contentSize": "var(--wp--custom--sizing--narrow)",
			"wideSize": "var(--wp--custom--sizing--wide)"
		}
	},
	"styles": {
		"blocks": {
			"core/button": {
				"border": {
					"radius": "0"
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--background)",
			"text": "var(--wp--preset--color--foreground)"
		},
		"elements": {
			"h1": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--pt-sans-narrow)",
					"fontWeight": "400",
					"lineHeight": "var(--wp--custom--typography--line-height--heading-1)",
					"fontSize": "var(--wp--custom--typography--font-size--heading-1)",
					"letterSpacing": "-0.025em"
				}
			},
			"h2": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--pt-sans-narrow)",
					"fontWeight": "400",
					"lineHeight": "var(--wp--custom--typography--line-height--heading-2)",
					"fontSize": "var(--wp--custom--typography--font-size--heading-2)",
					"letterSpacing": "-0.015em"
				}
			},
			"h3": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--pt-sans-narrow)",
					"fontWeight": "400",
					"lineHeight": "var(--wp--custom--typography--line-height--heading-3)",
					"fontSize": "var(--wp--custom--typography--font-size--heading-3)",
					"letterSpacing": "-0.01em"
				}
			},
			"h4": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--pt-sans-narrow)",
					"fontWeight": "400",
					"lineHeight": "var(--wp--custom--typography--line-height--heading-4)",
					"fontSize": "var(--wp--custom--typography--font-size--heading-4)",
					"letterSpacing": "-0.0075em"
				}
			},
			"h5": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--pt-sans-narrow)",
					"fontWeight": "400",
					"lineHeight": "var(--wp--custom--typography--line-height--heading-5)",
					"fontSize": "var(--wp--custom--typography--font-size--heading-5)",
					"letterSpacing": "-0.0025em"
				}
			},
			"h6": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--pt-sans-narrow)",
					"fontWeight": "400",
					"lineHeight": "var(--wp--custom--typography--line-height--heading-6)",
					"fontSize": "var(--wp--custom--typography--font-size--heading-6)"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--foreground)"
				}
			}
		},
		"spacing": {
			"blockGap": "1.5rem"
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--system-font)",
			"lineHeight": "var(--wp--custom--typography--line-height--medium)",
			"fontSize": "var(--wp--preset--font-size--medium)"
		}
	}
}
