{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"customTemplates": [],
	"templateParts": [
		{
			"name": "header",
			"title": "Header",
			"area": "header"
		},
		{
			"name": "footer",
			"title": "Foooter",
			"area": "footer"
		}
	],
	"settings": {
		"appearanceTools": true,
		"color": {
			"duotone": [],
			"gradients": [],
			"palette": [
				{
					"slug": "foreground",
					"color": "#000000",
					"name": "Foreground"
				},
				{
					"slug": "background",
					"color": "#ffffff",
					"name": "Background"
				},
				{
					"slug": "primary",
					"color": "#1a4548",
					"name": "Primary"
				},
				{
					"slug": "secondary",
					"color": "#ffe2c7",
					"name": "Secondary"
				},
				{
					"slug": "tertiary",
					"color": "#F6F6F6",
					"name": "Tertiary"
				}
			]
		},
		"custom": {
			"spacing": {
				"outer": "clamp(1.875rem, -7.1667rem + 14.5833vw, 4.5rem)",
				"rowGap": "clamp(1.5rem, -1.0833rem + 4.1667vw, 2.25rem)"
			},
			"sizing": {
				"narrow": "1000px",
				"wide": "1200px",
				"super": "1440px"
			},
			"padding": {
				"horizontal": "2.5rem"
			},
			"typography": {
				"font-size": {
					"heading-1": "clamp(2rem, 1.8750rem + 2.1875vw, 4.5rem)",
					"heading-2": "clamp(1.625rem, 1.3125rem + 0.7813vw, 2.25rem)",
					"heading-3": "clamp(1.375rem, 1.1875rem + 0.4688vw, 1.75rem)",
					"heading-4": "clamp(1.0625rem, 1.0313rem + 0.0781vw, 1.125rem)",
					"heading-5": "1rem",
					"heading-6": "0.875rem",
					"small": "1rem",
					"medium": "1.125rem",
					"large": "1.75rem",
					"x-large": "clamp(1.75rem, 3vw, 2.25rem)"
				},
				"line-height": {
					"heading-1": 1,
					"heading-2": 1.2,
					"heading-3": 1.2,
					"heading-4": 1.2,
					"heading-5": 1.2,
					"heading-6": 1.2,
					"small": 1.2,
					"medium": 1.4,
					"large": 1.4,
					"x-large": 1.4
				}
			}
		},
		"spacing": {
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"dropCap": false,
			"fontFamilies": [
				{
					"fontFamily": "-apple-system,BlinkMacSystemFont,\"Segoe UI\",Roboto,Oxygen-Sans,Ubuntu,Cantarell,\"Helvetica Neue\",sans-serif",
					"name": "System Font",
					"slug": "system-font"
				},
				{
					"fontFamily": "'PT Sans Narrow'",
					"name": "PT Sans Narrow",
					"slug": "pt-sans-narrow",
					"fontFace": [
						{
							"fontFamily": "PT Sans Narrow",
							"fontStyle": "normal",
							"fontWeight": "400",
							"fontDisplay": "swap",
							"src": [ "https://fonts.gstatic.com/s/ptsansnarrow/v18/BngRUXNadjH0qYEzV7ab-oWlsbCLwR2oefDofMY.woff2" ],
							"unicodeRange": "U+0460-052F, U+1C80-1C88, U+20B4, U+2DE0-2DFF, U+A640-A69F, U+FE2E-FE2F"
						},
